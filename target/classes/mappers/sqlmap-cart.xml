<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
   "-//mybatis.org//DTD Mapper 3.0//EN" 
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.pomaden.model.CartDAO">
	<insert id="insert" parameterType="cart">
		insert into cart 
		values
			(
			cart_seq.nextval,
			#{cart_member_id},
			#{cart_name},
			#{cart_img},
			#{cart_price},
			#{cart_sale},
			#{cart_count},
			#{cart_color},
			#{cart_size},
			#{cart_total}
			)
	</insert>
</mapper>