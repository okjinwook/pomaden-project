<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
   "-//mybatis.org//DTD Mapper 3.0//EN" 
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.pomaden.model.CartDAO">
	<insert id="insert" parameterType="cart">
		insert into cart 
		values
			(
			cart_seq.nextval,
			#{product_member_id},
			#{product_name},
			#{product_img},
			#{product_price},
			#{product_sale},
			#{product_category},
			#{product_count},
			#{product_color},
			#{product_size}
			)
	</insert>
</mapper>